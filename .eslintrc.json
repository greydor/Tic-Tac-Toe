{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-use-before-define": ["error", {
            "functions": false
        }],
	"no-param-reassign": [2, { "props": false }]
    },
    "globals": {
        "_": "readonly"
    }
}
